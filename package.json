{
  "name": "graphql-bridge-demo",
  "version": "0.1.0",
  "description": "Demo of the library graphql-bridge",
  "main": "src/index.js",
  "engines": {
    "node": "8.9.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npm run-script build",
    "start": "NODE_ENV=production node build/index.js",
    "dev": "NODE_ENV=development npm run-script nodemon-script",
    "nodemon-script":
      "nodemon src/index.js --exec babel-node --presets=es2015 --plugins transform-object-rest-spread",
    "build":
      "babel src -d build --presets=es2015 --plugins transform-object-rest-spread,transform-runtime"
  },
  "author": "Simon vom Eyser <simon.vom.eyser@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "apollo-fetch": "^0.7.0",
    "apollo-server-express": "^1.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.8.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-bridge": "file:graphql-bridge",
    "graphql-compose": "^2.9.6",
    "graphql-compose-json": "^1.0.2",
    "graphql-tools": "^2.12.0",
    "nodemon": "^1.12.1"
  },
  "dependencies": {}
}
